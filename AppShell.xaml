<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ThinkFast.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ThinkFast"
    xmlns:pages="clr-namespace:ThinkFast.Pages"
   
    Shell.FlyoutBehavior="Disabled"
  Shell.TabBarIsVisible="False"
    Title="ThinkFast"
    >



            <ShellContent
                Title="AiPage"
                ContentTemplate="{DataTemplate pages:AiPage}"
                Route="AiPage" />
      

      
       
       
            <ShellContent
                Title="FlashcardPage"
                ContentTemplate="{DataTemplate pages:FlashcardPage}"
                Route="FlashcardPage" />
       

        
        
            <ShellContent
                Title="TestPage"
                ContentTemplate="{DataTemplate pages:TestCardPage}"
                Route="TestCardPage" />
     


</Shell>
